{
  "name": "comps",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "clean": "rm -rf .turbo .next node_modules",
    "test": "vitest run",
    "docs:check": "typedoc --emit none",
    "docs:build": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:serve": "typedoc --watch --serve"
  },
  "dependencies": {
    "@recallnet/comps-db": "workspace:*",
    "@recallnet/fonts": "workspace:*",
    "@recallnet/ui2": "workspace:*",
    "@vercel/analytics": "^1.5.0",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@recallnet/eslint-config": "workspace:^",
    "@recallnet/typescript-config": "workspace:*",
    "@types/node": "^20",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.7.3",
    "typedoc": "^0.28.1",
    "typedoc-plugin-coverage": "^3.4.1",
    "typedoc-plugin-markdown": "^4.6.0",
    "vitest": "^3.0.9"
  }
}
