{
  "name": "api",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Solana Trading Simulator Server for hosting trading competitions",
  "main": "dist/src/index.js",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "start": "node dist/src/index.js",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "vitest --forceExit",
    "test:watch": "jest --watch",
    "test:e2e": "jest -c e2e/jest.config.js --forceExit",
    "test:e2e:watch": "jest -c e2e/jest.config.js --watch",
    "test:e2e:runner": "tsx e2e/run-tests.ts",
    "test:e2e:debug": "tsx e2e/debug-test.ts",
    "test:multi-chain": "jest src/services/providers/__tests__/multi-chain.provider.test.ts --forceExit",
    "test:multi-chain:e2e": "jest -c e2e/jest.config.js e2e/tests/price/multi-chain-provider.test.ts --forceExit",
    "db:init": "tsx scripts/setup-db.ts",
    "db:check": "tsx scripts/check-db.ts",
    "db:clean": "NODE_ENV=development tsx scripts/clean-db.ts",
    "db:reset": "npm run db:clean && npm run db:init",
    "db:drop-all": "NODE_ENV=development tsx scripts/drop-all-tables.ts",
    "generate:secrets": "tsx scripts/generate-secrets.ts",
    "setup:admin": "tsx scripts/setup-admin.ts",
    "setup:all": "tsx scripts/setup-everything.ts",
    "register:team": "tsx scripts/register-team.ts",
    "edit:team": "tsx scripts/edit-team.ts",
    "delete:team": "tsx scripts/delete-team.ts",
    "list:teams": "tsx scripts/list-teams.ts",
    "setup:competition": "tsx scripts/setup-competition.ts",
    "end:competition": "tsx scripts/end-competition.ts",
    "comp:status": "tsx scripts/competition-status.ts",
    "generate-openapi": "tsx -e \"const { swaggerSpec } = require('./src/config/swagger'); require('fs').writeFileSync('./openapi/openapi.json', JSON.stringify(swaggerSpec, null, 2));\"",
    "generate-markdown": "openapi-markdown -i ./openapi/openapi.json -o ./openapi/API.md",
    "generate-docs": "npm run generate-openapi && npm run generate-markdown"
  },
  "keywords": [
    "solana",
    "trading",
    "simulation",
    "competition"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^18.15.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.10.0",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.6.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@elizaos/core": "^0.25.9",
    "@recallnet/eslint-config": "workspace:^",
    "@recallnet/typescript-config": "workspace:*",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/pg": "^8.6.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.1",
    "axios": "^1.6.0",
    "openapi-markdown": "^1.2.3",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9"
  }
}
