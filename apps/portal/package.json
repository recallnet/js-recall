{
  "name": "portal",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "format": "prettier --write . --ignore-path=../../.prettierignore",
    "format:check": "prettier --check . --ignore-path=../../.prettierignore",
    "clean": "rm -rf .turbo .next node_modules",
    "test": "vitest run",
    "docs:check": "typedoc --emit none",
    "docs:build": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:serve": "typedoc --watch --serve"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.0.0",
    "@rainbow-me/rainbowkit": "^2.2.4",
    "@recallnet/address-utils": "workspace:*",
    "@recallnet/chains": "workspace:*",
    "@recallnet/conversions": "workspace:*",
    "@recallnet/fonts": "workspace:*",
    "@recallnet/react": "workspace:*",
    "@recallnet/ui": "workspace:*",
    "@tanstack/react-query": "^5.64.2",
    "@vercel/analytics": "^1.5.0",
    "javascript-time-ago": "^2.5.11",
    "lucide-react": "0.456.0",
    "moment": "^2.30.1",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.54.2",
    "react-time-ago": "^7.3.3",
    "viem": "^2.28.1",
    "wagmi": "^2.15.2",
    "zod": "^3.25.17"
  },
  "devDependencies": {
    "@recallnet/eslint-config": "workspace:^",
    "@recallnet/typescript-config": "workspace:*",
    "@types/node": "^20",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "prettier": "^3.5.3",
    "typedoc": "^0.28.1",
    "typedoc-plugin-coverage": "^3.4.1",
    "typedoc-plugin-markdown": "^4.6.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.9"
  }
}
