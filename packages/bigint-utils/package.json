{
  "name": "@recall/bigint-utils",
  "version": "0.0.0",
  "license": "MIT AND Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js",
      "require": "./dist/constants.cjs"
    },
    "./conversions": {
      "types": "./dist/conversions.d.ts",
      "import": "./dist/conversions.js",
      "require": "./dist/conversions.cjs"
    },
    "./format-atto-rcl": {
      "types": "./dist/format-atto-rcl.d.ts",
      "import": "./dist/format-atto-rcl.js",
      "require": "./dist/format-atto-rcl.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "devDependencies": {
    "@recall/eslint-config": "workspace:*",
    "@recall/typescript-config": "workspace:*",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "dnum": "^2.14.0"
  }
}
