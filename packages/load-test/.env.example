# Load Testing Environment Configuration
# Copy to .env and update with your values

# API Configuration
# Target API endpoint for load testing
API_HOST=

# Admin API key (required for creating competitions and agents)
ADMIN_API_KEY=your_admin_api_key_here

# Test Configuration
# Number of agents to create for baseline test (stress tests use fixed counts)
AGENTS_COUNT=5

# Optional: Stress Test Parameters (can be overridden via CLI)
# TEST_DURATION=60  # Test duration in seconds (default: 60)
# REQUEST_RATE=8  # Request rate per second (default: 8)
# TRADE_AMOUNT=0.1  # Trade amount in dollars (default: 0.1)

# Optional: Sentry Configuration (if monitoring performance)
# SENTRY_DSN=your_sentry_dsn_here
# SENTRY_ORG=your_sentry_org_here
# SENTRY_PROJECT_ID=your_sentry_project_id_here
# SENTRY_TRACES_SAMPLE_RATE=0.01  # 1% sampling for load tests (default)
# SENTRY_SAMPLE_REQUEST=0.01  # 1% sampling for requests (default)
