# Load Testing Environment Configuration
# Copy to .env and update with your values

# API Configuration
# Target API endpoint for load testing
API_HOST=

# Admin API key (required for creating competitions and agents)
ADMIN_API_KEY=your_admin_api_key_here

# Test Configuration
# Number of agents to create for baseline test (stress tests use fixed counts)
AGENTS_COUNT=5

# Number of trades each agent will execute (for legacy configs)
TRADES_COUNT=100

# Token Addresses (EVM - Ethereum Mainnet)
# These can be overridden for testing on different chains
# USDC_TOKEN=0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48
# WETH_TOKEN=0xc02aaa39b223fe8d0a0e5c4f27ead9083c756cc2

# Optional: Debug Mode
# DEBUG=true  # Enable verbose logging

# Disable Rate Limiter (useful for load testing)
DISABLE_RATE_LIMITER=true

# Optional: Sentry Configuration (if monitoring performance)
# SENTRY_DSN=your_sentry_dsn_here
# SENTRY_TRACES_SAMPLE_RATE=0.01  # 1% sampling for load tests
