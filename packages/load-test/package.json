{
  "name": "@recallnet/load-test",
  "version": "0.0.1",
  "description": "Agent trading load testing for the RecallNet platform",
  "license": "MIT AND Apache-2.0",
  "private": true,
  "scripts": {
    "analyze": "tsx src/agent-trading/report-analyzer.ts",
    "analyze:latest": "tsx src/agent-trading/report-analyzer.ts $(ls -t reports/*.json 2>/dev/null | head -1)",
    "analyze:consolidate": "tsx src/agent-trading/report-analyzer.ts --consolidate",
    "dev": "tsc --watch",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write . --ignore-path=../../.prettierignore",
    "format:check": "prettier --check . --ignore-path=../../.prettierignore",
    "clean": "rm -rf .turbo node_modules dist src/**/dist reports/*"
  },
  "dependencies": {
    "@ngneat/falso": "^8.0.2",
    "@sentry/node": "^10.5.0",
    "@sentry/tracing": "^7.120.4",
    "artillery": "^2.0.24",
    "artillery-plugin-ensure": "^1.20.0",
    "artillery-plugin-metrics-by-endpoint": "^1.20.0",
    "artillery-plugin-publish-metrics": "^2.31.0",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@recallnet/eslint-config": "workspace:*",
    "@recallnet/typescript-config": "workspace:*",
    "@types/node": "^24.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3"
  }
}
