# @recallnet/api-sdk SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: APISDK_
#
# Security environment variables:
APISDK_BEARER_AUTH=your_bearer_auth_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
APISDK_DEBUG=false
