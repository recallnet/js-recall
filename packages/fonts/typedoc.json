{
  "$schema": "https://typedoc.org/schema.json",
  "entryPointStrategy": "resolve",
  "entryPoints": [
    "./scripts/open-fonts.ts.template",
    "./scripts/private-fonts.ts.template"
  ],
  "out": "docs",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-plugin-coverage"
  ],
  "theme": "default",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Functions",
    "Types",
    "Interfaces",
    "Variables"
  ],
  "searchInComments": true,
  "readme": "README.md",
  "validation": {
    "invalidLink": true,
    "notDocumented": true
  },
  "coverageLabel": "docs",
  "coverageColor": "brightgreen",
  "coverageOutputType": "all"
}