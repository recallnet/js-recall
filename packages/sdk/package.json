{
  "name": "@recall/sdk",
  "version": "0.0.0",
  "description": "Core SDK for building with Recall EVM contracts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hokunet/js-recall.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/hokunet/js-recall#readme",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "license": "MIT AND Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "dist": "./dist"
  },
  "files": [
    "dist/**/*.js(.map)",
    "dist/**/*.d.ts",
    "dist/**/package.json",
    "src/**/*.ts"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "npm run test:mocha",
    "test:mocha": "mocha --exit",
    "test:ci": "npm run coverage",
    "coverage": "c8 --exclude test --exclude dist mocha --exit",
    "docs": "typedoc --entryPoints src/index.ts",
    "clean": "rm -rf dist coverage docs",
    "build:esm": "npx tsc",
    "build": "npm run build:esm"
  },
  "dependencies": {
    "@recall/fvm": "^0.0.0",
    "@noble/hashes": "^1.5.0",
    "@number0/iroh": "^0.28.1",
    "@sindresorhus/fnv1a": "^3.1.0",
    "file-type": "^19.6.0",
    "uint8arrays": "^5.1.0",
    "viem": "^2.21.37"
  },
  "devDependencies": {
    "tempy": "^3.1.0"
  }
}
