{
  "name": "@recall/sdkx",
  "version": "0.0.0",
  "license": "MIT AND Apache-2.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./chains": {
      "types": "./dist/chains.d.ts",
      "import": "./dist/chains.mjs",
      "require": "./dist/chains.cjs"
    },
    "./react/credits": {
      "types": "./dist/react/credits.d.ts",
      "import": "./dist/react/credits.mjs",
      "require": "./dist/react/credits.cjs"
    },
    "./actions/credits": {
      "types": "./dist/actions/credits.d.ts",
      "import": "./dist/actions/credits.mjs",
      "require": "./dist/actions/credits.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "devDependencies": {
    "@recall/eslint-config": "workspace:*",
    "@recall/typescript-config": "workspace:*",
    "@types/react": "^19.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@recall/contracts": "workspace:*",
    "@tanstack/react-query": "^5.64.2",
    "react": "^19.0.0",
    "viem": "^2.22.9",
    "wagmi": "^2.14.8"
  }
}
