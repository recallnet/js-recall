{
  "name": "@recallnet/db-schema",
  "version": "0.0.0",
  "license": "MIT AND Apache-2.0",
  "private": true,
  "description": "Recall Drizzle schema.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/recallnet/js-recall.git",
    "directory": "packages/db-schema"
  },
  "homepage": "https://github.com/recallnet/js-recall#readme",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./coders": "./src/coders/index.ts",
    "./errors": "./src/errors.ts",
    "./repositories/admin": "./src/repositories/admin.ts",
    "./repositories/agent-nonce": "./src/repositories/agent-nonce.ts",
    "./repositories/agent": "./src/repositories/agent.ts",
    "./repositories/agent-score": "./src/repositories/agent-score.ts",
    "./repositories/balance": "./src/repositories/balance.ts",
    "./repositories/boost": "./src/repositories/boost.ts",
    "./repositories/competition": "./src/repositories/competition.ts",
    "./repositories/competition-rewards": "./src/repositories/competition-rewards.ts",
    "./repositories/leaderboard": "./src/repositories/leaderboard.ts",
    "./repositories/rewards": "./src/repositories/rewards.ts",
    "./repositories/trade": "./src/repositories/trade.ts",
    "./repositories/trading-constraints": "./src/repositories/trading-constraints.ts",
    "./repositories/user": "./src/repositories/user.ts",
    "./repositories/vote": "./src/repositories/vote.ts",
    "./types": "./src/types.ts",
    "./core/defs": "./src/core/defs.ts",
    "./core/relations": "./src/core/relations.ts",
    "./core/types": "./src/core/types.ts",
    "./indexing/defs": "./src/indexing/defs.ts",
    "./indexing/types": "./src/indexing/types.ts",
    "./ranking/defs": "./src/ranking/defs.ts",
    "./ranking/relations": "./src/ranking/relations.ts",
    "./ranking/types": "./src/ranking/types.ts",
    "./trading/defs": "./src/trading/defs.ts",
    "./trading/relations": "./src/trading/relations.ts",
    "./trading/types": "./src/trading/types.ts",
    "./voting/defs": "./src/voting/defs.ts",
    "./voting/relations": "./src/voting/relations.ts",
    "./voting/types": "./src/voting/types.ts"
  },
  "scripts": {
    "test:integration": "vitest run --project integration",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write . --ignore-path=../../.prettierignore",
    "format:check": "prettier --check . --ignore-path=../../.prettierignore",
    "docs:check": "typedoc --emit none",
    "docs:build": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:serve": "typedoc --watch --serve"
  },
  "dependencies": {
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.8.2",
    "pino": "^9.7.0",
    "zod": "^3.25.17"
  },
  "devDependencies": {
    "@recallnet/eslint-config": "workspace:*",
    "@recallnet/typescript-config": "workspace:*",
    "@types/node": "^24.3.0",
    "drizzle-kit": "^0.31.1",
    "tsup": "^8.3.6",
    "typedoc": "^0.28.1",
    "typedoc-plugin-coverage": "^3.4.1",
    "typedoc-plugin-markdown": "^4.6.0",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
