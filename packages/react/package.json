  "devDependencies": {
    "@recallnet/eslint-config": "workspace:*",
    "@recallnet/typescript-config": "workspace:*",
    "@types/react": "^19.0.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@recallnet/chains": "workspace:*",
    "@recallnet/contracts": "workspace:*",
    "@tanstack/react-query": "^5.64.2",
    "axios": "^1.7.9",
