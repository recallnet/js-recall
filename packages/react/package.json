    "./actions/credits": {
      "import": {
        "types": "./dist/actions/credits.d.ts",
        "default": "./dist/actions/credits.js"
      },
      "require": {
        "types": "./dist/actions/credits.d.cts",
        "default": "./dist/actions/credits.cjs"
      }
    }
